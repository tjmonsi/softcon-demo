{
  "name": "base-js-template",
  "version": "0.0.1",
  "description": "Base template for all JS projects that I do",
  "main": "index.js",
  "scripts": {
    "commit": "npx git-cz",
    "semistandard": "npx semistandard",
    "test": "npx npm-run-all --serial semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmonsi/base-js-template.git"
  },
  "keywords": [
    "base",
    "js",
    "project"
  ],
  "author": "TJ Monserrat",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tjmonsi/base-js-template/issues"
  },
  "homepage": "https://github.com/tjmonsi/base-js-template#readme",
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "babel-eslint": "10.0.3",
    "conventional-changelog": "3.1.12",
    "eslint": "6.6.0",
    "eslint-config-semistandard": "15.0.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-mocha": "6.2.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "3.0.9",
    "npm-run-all": "4.1.5",
    "semistandard": "14.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "after",
      "before"
    ]
  }
}
